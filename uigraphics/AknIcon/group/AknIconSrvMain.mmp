EPOCPROCESSPRIORITY high
#ifdef WINS
EPOCHEAPSIZE    0x1000 0x500000	
#else
EPOCHEAPSIZE    0x1000 0x1000000	
#endif

EPOCSTACKSIZE   0x3000


